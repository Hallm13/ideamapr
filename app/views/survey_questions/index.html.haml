= render partial: 'admin_add_button', locals: {button_text: t('survey_question.add_button'), button_target: new_survey_question_url}
      
- if @questions.size > 0
  .entity-set
    .row.table-headers
      .col-xs-9
        Question Title
      .col-xs-3
        Question Type

  - @questions.each do |question|
    .row.question-row
      .col-xs-9.goto-edit{'data-goto-target' => edit_survey_question_path(question)}
        .cell-1
          = question.title
      .col-xs-3
        .cell-1
          = SurveyQuestion::QuestionType.name(question.question_type) || 'Not set'
