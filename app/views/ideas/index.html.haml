%button.btn.btn-primary
  = link_to '+ Add An Idea', new_idea_url
  
- @ideas.each do |idea|
  .row.question-row
    .col-xs-10
      .cell-1.clickable-box{'data-link' => idea_url(idea)}
        = idea.title
    .col-xs-2
      "> Edit"

- if @groupable
  %button.btn.btn-primary#add-multi-select
    Add To Groupable
